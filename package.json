  "main": "server.js",
  "scripts": {
    "build": "sh build.sh",
    "deploy": "sh deploy.sh",
    "deploy-back-end": "kubectl create -f back-end/index.yaml",
    "deploy-front-end": "kubectl create -f front-end/index.yaml",
    "deploy-volumes": "kubectl create -f volumes/index.yaml",
    "destroy": "sh destroy.sh",
    "destroy-back-end": "kubectl delete -f back-end/index.yaml",
    "destroy-front-end": "kubectl delete -f front-end/index.yaml",
    "destroy-db": "kubectl delete -f db/index.yaml",
    "destroy-volumes": "kubectl delete -f volumes/index.yaml",
    "update": "sh update.sh",
    "setup": "npm run env && npm run build && npm run deploy",
    "test": "node test.js",
    "start": "node ."
